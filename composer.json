{
  "name": "thefrosty/wp-plugin",
  "description": "A base WordPress plugin library.",
  "license": "MIT",
  "version": "0.4.1",
  "homepage": "https://github.com/thefrosty/wp-plugin",
  "authors": [
    {
      "name": "Austin Passy",
      "email": "thefrosty@users.noreply.github.com",
      "homepage": "https://austin.passy.co",
      "role": "Developer"
    },
    {
      "name": "Brady Vercher",
      "email": "brady@blazersix.com",
      "homepage": "https://www.cedaro.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/thefrosty/wp-plugin/issues",
    "source": "https://github.com/thefrosty/wp-plugin"
  },
  "require": {
    "php": ">=7.0.1",
    "pimple/pimple": "^3.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*@stable"
  },
  "suggest": {
    "thefrosty/wp-utilities": "A library containing additional WordPress development resources."
  },
  "autoload": {
    "psr-4": {
      "TheFrosty\\WP\\Plugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TheFrosty\\WP\\Plugin\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --coverage-html coverage"
  }
}
